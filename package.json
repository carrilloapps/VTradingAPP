{
  "name": "vtrading",
  "version": "1.0.6",
  "description": "Aplicación de monitoreo financiero y trading para el mercado venezolano",
  "author": "José Carrillo",
  "license": "UNLICENSED",
  "private": true,
  "keywords": [
    "react-native",
    "finance",
    "trading",
    "venezuela",
    "currency",
    "exchange-rates"
  ],
  "homepage": "https://vtrading.app",
  "bugs": {
    "email": "info@vtrading.app"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:run": "react-native run-android --mode=release",
    "android:release": "node scripts/android-manager.js bundleRelease",
    "android:apk": "node scripts/android-manager.js assembleRelease",
    "android:signingReport": "node scripts/android-manager.js signingReport",
    "android:clean": "node scripts/android-manager.js clean",
    "ios": "react-native run-ios",
    "ios:pods": "cd ios && pod install && cd ..",
    "ios:clean": "cd ios && xcodebuild clean -workspace VTradingAPP.xcworkspace -scheme VTradingAPP && cd ..",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:update": "jest -u",
    "test:ci": "jest --ci --coverage",
    "tsc": "tsc --noEmit",
    "format": "prettier --write .",
    "check-deps": "npm ls --depth=0 2>&1 | Select-String -Pattern 'UNMET PEER DEPENDENCY|missing'"
  },
  "dependencies": {
